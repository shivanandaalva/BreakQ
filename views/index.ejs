<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8">
    <title>BreakQ</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
  </head>
  <body>
    <nav class="navbar fixed-top navbar-dark bg-dark justify-content-center">
      <a class="navbar-brand" href="#">BreakQ</a>
    </nav>
    <br>
    <div class="content" id="content">
      <br>
      <div class="row row-cols-4 row-cols-md-4 " >
      <% data.forEach(function(data){ %>
     
      <div class="col mb-4 "  id="">
          <div class="card h-80" onclick="data(id)" id="<%=data.productId%>">
            <img src="download.png" class="img-fluid" alt=" "/>
        <h1><%=data.productName%></h1>
        <h1><%=data.productCost%></h1>
   
      </div> 
        </div> 
        <% }); %>
      </div>
    </div>
    <script>
      function data(id){
        window.location.href = "/product/"+id;
      }
       
    </script>
    <style>
      body{
      background-color:orange;
      padding:50px;
      }

      </style>
  </body>
</html>
